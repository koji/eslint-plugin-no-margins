{
  "name": "eslint-plugin-no-margins",
  "version": "0.0.1",
  "description": "ESLint plugin to enforce consistent spacing by preventing the use of margin properties in CSS-in-JS. Helps maintain consistent layout patterns across your React components.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "eslint",
    "eslint-plugin"
  ],
  "author": "koji",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koji/eslint-plugin-no-margins.git"
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "publish": "npm publish"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.5",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.10.6",
    "@typescript-eslint/experimental-utils": "^5.62.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@typescript-eslint/utils": "^8.20.0",
    "@vitest/coverage-istanbul": "2.1.8",
    "eslint": "^9.18.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
